{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "author": "pana",
  "homepage_url": "https://github.com/LightAPIs/reload-tabs-of-the-same-domain",
  "description": "__MSG_extDes__",
  "default_locale": "en",
  "icons": {
